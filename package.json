  "dependencies": {
    "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "^3.0.2",
    "@module-federation/typescript": "^3.1.1",
    "concurrently": "^8.2.2",
    "decap-cms-app": "^3.0.12",
    "decap-server": "^3.0.1",
    "gatsby": "^4.25.7",
    "gatsby-plugin-decap-cms": "^4.0.4",
    "gatsby-plugin-federation": "^2.5.0",
    "gatsby-plugin-image": "^2.24.0",
    "gatsby-plugin-sharp": "^4.24.0",
    "gatsby-remark-copy-linked-files": "^5.24.0",
    "gatsby-remark-images": "^6.24.0",
    "gatsby-remark-prismjs": "^6.24.0",
    "gatsby-remark-responsive-iframe": "^5.24.0",
    "gatsby-remark-smartypants": "^5.24.0",
    "gatsby-source-filesystem": "^4.24.0",
    "gatsby-transformer-json": "^4.25.0",
    "gatsby-transformer-remark": "^5.24.0",
    "gatsby-transformer-sharp": "^4.24.0",
    "gatsby-transformer-yaml": "^4.25.0",
    "html-webpack-plugin": "^5.5.4",
    "marked": "^11.1.1",
    "prismjs": "^1.29.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-loader": "^9.5.1",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.75"
  },
  "devDependencies": {
    "@types/react-dom": "^18.2.19",
    "prettier": "^2.7.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "homepage": "https://github.com/strenuus/n360-cms-content#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strenuus/n360-cms-content.git"
  },
  "scripts": {
    "build": "npx gatsby build",
    "docker-build": "docker compose run --rm web npx gatsby build",
    "develop": "gatsby develop",
    "bounce": "docker compose down --rmi all && docker compose up --build --detach",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "concurrently -k \"decap-server\" \"gatsby develop\"",
    "start:mf": "npm run clean && npm run build && npm run serve:mf",
    "serve": "gatsby serve",
    "serve:mf": "webpack serve --mode=development --config=config/webpack.config.js",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  }
}
